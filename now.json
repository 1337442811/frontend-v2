{
  "version": 2,
  "public": true,
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/PenguinStats/api/(.*)",
      "destination": "https://penguin-stats.io/PenguinStats/api/$1"
    }
  ]
}